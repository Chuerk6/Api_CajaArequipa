<Window x:Class="Presentacion.SucursalWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Presentacion"
        mc:Ignorable="d"
        Title="SucursalWindow" Height="337" Width="352">
    <Window.Background>
        <ImageBrush ImageSource="/icono-agencias.png"/>
    </Window.Background>
    <Grid Margin="0,0,23,23">
        <DataGrid x:Name="dgSucursales" HorizontalAlignment="Left" Height="191" VerticalAlignment="Top" Width="252" Margin="49,45,0,0" SelectionChanged="dgSucursales_SelectionChanged">
            <DataGrid.Background>
                <ImageBrush/>
            </DataGrid.Background>
        </DataGrid>

    </Grid>
</Window>
